        xmlhttp.open("GET", url, true);
        xmlhttp.send();
    }

    function getCookie(name) {
        var value = "; " + document.cookie;
        var parts = value.split("; " + name + "=");
        if (parts.length == 2) {
            return parts.pop().split(";").shift();
        }
    }

    if (typeof getCookie("cookies_populated") == "undefined"
        || typeof getCookie("CACHED_FRONT_FORM_KEY") == "undefined"
    ) {
        requestCookies();
